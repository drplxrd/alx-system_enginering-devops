# Configures Nginx to serve Airbnb_clone_v3 api/states

server {
        listen 80;
        listen [::]:80;
        add_header X-Served-By $HOSTNAME;

        # Server IP
        server_name 34.227.91.106;

        # Serve api route  from Airbnb_clone_v3
        location /api {
                proxy_pass http://127.0.0.1:5003;
        }
        # Error handling
        error_page 404 /404.html;
        location /404 {
        root /var/www/html;
        internal;
    }
}



